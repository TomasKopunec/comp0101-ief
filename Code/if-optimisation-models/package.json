{
  "name": "@grnsft/if-optimisation-models",
  "version": "0.1.0",
  "description": "Optimisation models for IF (Impact Framework)",
  "main": "build/index.js",
  "scripts": {
    "start": "./run.sh",
    "build": "rm -rf build && tsc --project tsconfig.build.json",
    "install2if": "./install2if.sh local",
    "install2if:local": "./install2if.sh local",
    "install2if:dev": "./install2if.sh dev",
    "if:local": "yarn if",
    "if:dev": "cd ../if && yarn if",
    "intall-if": "npm run install2if && yarn if",
    "install-and-exec:local": "./install2if.sh local && yarn if",
    "install-and-exec:dev": "./install2if.sh dev && cd ../if && yarn if",
    "test": "jest --verbose"
  },
  "keywords": [
    "impact-framework",
    "typescript"
  ],
  "author": "UCL IEF Team",
  "license": "ISC",
  "devDependencies": {
    "@grnsft/if": "github:Green-Software-Foundation/if#functional-architecture",
    "@grnsft/if-models": "github:Green-Software-Foundation/if-models#refactor-to-pure-functions",
    "@grnsft/if-unofficial-models": "^0.1.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "dotenv": "^16.3.2",
    "js-yaml": "^4.1.0",
    "qs": "^6.11.2",
    "typescript-cubic-spline": "^1.0.0",
    "zod": "^3.22.4"
  }
}
