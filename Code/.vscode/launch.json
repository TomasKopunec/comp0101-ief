{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "IF: Launch",
            "skipFiles": [
                //"<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/if",
            "runtimeVersion": "18",
            "program": "${workspaceFolder}/if/src/index.ts",
            "preLaunchTask": "install2if:dev",
            "outFiles": [
                "${workspaceFolder}/if/**/*.js",
                "${workspaceFolder}/if-optimisation-models/**/*.js",
                "${workspaceFolder}/if/**/*.ts",
                "${workspaceFolder}/if-optimisation-models/**/*.ts"
            ],
            "args": [
                "--impl",
                "./examples/low-util.yml"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "IF: Launch (No Install)",
            "skipFiles": [
                //"<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/if",
            "runtimeVersion": "18",
            "program": "${workspaceFolder}/if/src/index.ts",
            "preLaunchTask": "build-with-copy",
            "outFiles": [
                "${workspaceFolder}/if/**/*.js",
                "${workspaceFolder}/if-optimisation-models/**/*.js",
                "${workspaceFolder}/if/**/*.ts",
                "${workspaceFolder}/if-optimisation-models/**/*.ts"
            ],
            "args": [
                "--impl",
                "./examples/low-util.yml"
            ]
        }
    ]
}